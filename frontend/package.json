{
    "name": "frontend",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite --host",
        "build": "tsc && vite build --sourcemap true",
        "preview": "vite preview",
        "format": "prettier --config .prettierrc.json --write src/",
        "gen-api": "bash scripts/gen-api.sh",
        "ci": "prettier --check src/ && eslint 'src/**/*.{ts,tsx}' --report-unused-disable-directives --max-warnings 0"
    },
    "dependencies": {
        "@headlessui/react": "^2.2.0",
        "@heroicons/react": "^2.2.0",
        "@tailwindcss/vite": "^4.0.7",
        "@tanstack/react-router": "^1.112.0",
        "@tanstack/router-plugin": "^1.112.0",
        "clsx": "^2.1.1",
        "framer-motion": "^12.5.0",
        "i18next-browser-languagedetector": "^8.0.4",
        "i18next-http-backend": "^3.0.2",
        "prettier-plugin-tailwindcss": "^0.6.11",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-i18next": "^15.4.1",
        "react-toastify": "^11.0.5",
        "tailwindcss": "^4.0.7",
        "vite-plugin-svgr": "^4.3.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.19.0",
        "@types/react": "^19.0.8",
        "@types/react-dom": "^19.0.3",
        "@vitejs/plugin-react-swc": "^3.5.0",
        "eslint": "^9.19.0",
        "eslint-plugin-react-hooks": "^5.0.0",
        "eslint-plugin-react-refresh": "^0.4.18",
        "globals": "^15.14.0",
        "typescript": "~5.7.2",
        "typescript-eslint": "^8.22.0",
        "vite": "^6.2.3"
    }
}
