                    "super_admin": {
                        "description": "Whether the user is a super admin.",
                        "type": "boolean"
                    }
                },
                "required": [
                    "admins",
                    "member",
                    "super_admin"
                ]
            },
            "SchemaDateTime": {
                "example": "1970-01-01T00:00:00.0Z",
                "type": "string",
                "format": "date-time"
            },
            "SignInRequest": {
                "description": "Sign in request",
                "type": "object",
                "properties": {
                    "password": {
                        "description": "Password",
